{
    "services": [
      {
        "name": "mqtt-broker",
        "image": "emqx/emqx:latest",
        "env": {
          "EMQX_NAME": "mqtt-broker"
        },
        "ports": {
          "1883": {
            "protocol": "tcp"
          },
          "8081": {
            "protocol": "tcp"
          }
        }
      }
    ]
  }
  